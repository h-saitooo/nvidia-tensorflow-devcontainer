{
  "name": "Nvidia Tensorflow",
  "dockerComposeFile": "compose.yml",
  "service": "nvidia-tensorflow",
  "workspaceFolder": "/workspace",
  "forwardPorts": [8888],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ],
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      }
    }
  }
}